FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


solvers
{

    "(U|rho|rhoU|rhoE|e|h)"
    {
        solver          PBiCGStab;
        preconditioner  FDIC;
        tolerance       1e-12;
        relTol          0;
	minIter		2;
    }

    "(k|omega|nuTilda)"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-12;
        relTol          0;
        minIter         2;
    }

}











