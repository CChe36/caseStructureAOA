FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoCentralFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         .1;

deltaT          5e-9;

writeControl    adjustable;

writeInterval   1e-6; // changed to 1e-3 for visual representations

// need to update some function objects to write to 1e-3 as well, can't write isentopic(p) when its not written to folder

purgeWrite      5; // for visual representations, change to 45 to see ~3 buzz cycles for higher freq, kept low for storage management

writeFormat     ascii;

writePrecision  9;

writeCompression off;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       .0025;

functions
{
    #includeFunc "yPlus"
    #includeFunc "MachNo"

    #includeFunc "probe0"
    #includeFunc "probe1"
    #includeFunc "probe2"
    #includeFunc "probe3"
    #includeFunc "probe4"
    #includeFunc "probe90"
    
    #includeFunc "bottomSurfacePres"
    #includeFunc "cowlSurfacePres"

    #includeFunc "inlet52MFR"
    #includeFunc "outlet90MFR"

    #includeFunc "inlet52Avg"
    #includeFunc "outlet90Avg"

    #includeFunc "outlet90Max"
    #includeFunc "outlet90Min"

    #includeFunc "pressureIsentropic"

    #includeFunc "inletIsentropicAvg"
    #includeFunc "inlet52IsentropicAvg"
    #includeFunc "outlet90IsentropicAvg"
}


