FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];  // kg m s K mol A cd

internalField   uniform (0 0 0);

boundaryField
{
    inlet1
    {
    type                freestreamVelocity;
    freestreamValue     uniform (543.174 -37.98 0); // Mach 2.2
    value               uniform (543.174 -37.98 0); // alpha = -4
    }

    outlet1
    {
        type            zeroGradient;
    }

    outlet2
    {
        type            zeroGradient;
    }
    
    bottom 
    {
        type            zeroGradient;
    }
    
    "(wall1|wall2).*"
    {
        type            noSlip;
    }

    top
    {
        type            zeroGradient;
    };
    

    defaultFaces
    {
        type            empty;
    }
}


